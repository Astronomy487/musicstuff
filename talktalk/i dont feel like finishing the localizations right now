	getExplicit: function(key) {
		if (languageFull.startsWith("zh-Hant")) return localizer.explicit[key]["zh-Hant"] || localizer.explicit[key]["zh"] || localizer.explicit[key]["en"];
		return localizer.explicit[key][languageFull] || localizer.explicit[key][language] || localizer.explicit[key]["en"];
	},
	formatSongMembershipLine: function(element, trackNumber, albumTitle) {
		let text = {
			zh: "#a 第#n首",
			hi: "#a पर गाना #n",
			es: "Pista #n de #a",
			ar: "المقطع #n على ألبوم #a",
			fr: "Titre #n de l’album #a",
			bn: "#a-তে #n ট্র্যাক",
			pt: "Faixa #n no #a",
			ru: "Трек #n на #a",
			id: "Lagu ke-#n pada #a",
			ur: "#a پر ٹریک #n",
			de: "Titel #n von #a",
			ja: "#aの#n曲目",
			it: "Traccia #n di #a",
			nl: "Track #n op #a",
			pl: "Utwór #n na #a",
			tr: "#a albümünde #n. parça",
			fa: "آهنگ #n در آلبوم #a",
			vi: "Ca khúc #n trên album #a",
			cs: "Skladba #n na #a",
			ko: "#a의 #n번 트랙",
			uk: "Трек #n на #a",
			hu: "#n. track a #a albumon",
			sv: "Spår #n på #a",
			ro: "Melodie #n de pe #a",
			el: "Κομμάτι #n στο #a",
			he: "שיר #n מתוך #a",
			da: "Nummer #n på #a",
			fi: "Kappale #n albumilla #a",
			th: "แทร็กที่ #n ใน #a",
			nb: "Spor #n på #a",
			ms: "Trek #n pada #a",
			eo: "Trako #n en #a"
		}[language] || "Track #n on #a";
		element.innerText = text.replace("#n", localizer.processNumber(trackNumber)).replace("#a", albumTitle);
		if (localizer.rtl[language]) element.setAttribute("dir", "rtl");
		
	},
	formatSongMembershipLineBonus: function(element, albumTitle) {
		let text = {
			zh: "#a 附加曲目",
			hi: "#a पर बोनस ट्रैक",
			es: "Pista extra de #a",
			ar: "مقطع إضافي على ألبوم #a",
			fr: "Titre bonus de l’album #a",
			bn: "#a-তে বোনাস ট্র্যাক",
			pt: "Faixa bônus no #a",
			ru: "Бонус-трек на #a",
			id: "Lagu bonus pada #a",
			ur: "#a پر بونس ٹریک",
			de: "Bonustrack auf #a",
			ja: "#aのボーナストラック",
			it: "Traccia bonus su #a",
			nl: "Bonustrack op #a",
			pl: "Bonusowy utwór na #a",
			tr: "#a albümünde bonus parça",
			fa: "قطعه جایزه در آلبوم #a",
			vi: "Bài hát bonus trên album #a",
			cs: "Bonusová skladba na #a",
			ko: "#a의 보너스 트랙",
			uk: "Бонус-трек на #a",
			hu: "Bónuszdal a #a albumon",
			sv: "Bonusspår på #a",
			ro: "Melodie bonus pe #a",
			el: "Bonus κομμάτι στο #a",
			he: "שיר בונוס באלבום #a",
			da: "Bonustrack på #a",
			fi: "Bonuskappale albumilla #a",
			th: "แทร็กโบนัสใน #a",
			nb: "Bonusspor på #a",
			ms: "Trek bonus pada #a",
			eo: "Bonustrako en #a"
		}[language] || "Bonus track on #a";
		element.innerText = text.replace("#a", albumTitle);
		if (localizer.rtl[language]) element.setAttribute("dir", "rtl");
	},
};