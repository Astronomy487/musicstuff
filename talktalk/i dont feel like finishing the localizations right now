i have to update the website for the july 18 release but i was in the middle of moving everything to using Talktalk

let languageFull = navigator.language || "en-US";
let language = languageFull.includes("-") ? languageFull.substring(0, languageFull.indexOf("-")) : languageFull;

let localizer = {
	rtl: {
		ar: true,
		ur: true,
		fa: true,
		he: true
	},
	noSpaces: {
		ja: true,
		zh: true
	},
	processNumber(n, minDigits = 1) {
		return new Intl.NumberFormat(languageFull, {minimumIntegerDigits: minDigits}).format(n);
	},
	formatDate: function(element, date) {
		date = new Date(parseInt(date.substring(0, 4)), parseInt(date.substring(5, 7))-1, parseInt(date.substring(8, 10)));
		element.innerText = date.toLocaleDateString(languageFull, {month: "short", day: "numeric", year: "numeric"});
		if (localizer.rtl[language]) element.setAttribute("dir", "rtl");
	},
	formatDuration: function(element, seconds) {
		element.innerText = new Intl.DurationFormat(languageFull, {style: "narrow"}).format({
			hours: Math.floor(seconds/3600),
			minutes: Math.floor(seconds/60)%60,
			seconds: seconds%60
		});
		if (localizer.rtl[language]) element.setAttribute("dir", "rtl");
	},
	formatTrackCount: function(element, n) {
		element.innerText = ({
			en: "#n tracks", es: "#n pistas", de: "#n Titel", ja: "#n曲", fr: "#n titres",
			pt: "#n faixas", ru: "#n треков", it: "#n brani", nl: "#n nummers", pl: "#n utworów",
			tr: "#n parça", zh: "#n曲目", vi: "#n ca khúc", id: "#n lagu",
			cs: "#n skladeb", ko: "#n곡", uk: "#n треків", hu: "#n dal", sv: "#n spår",
			ro: "#n piese", el: "#n κομμάτια", da: "#n numre",
			ar: "#n مسارات", he: "#n שירים", fa: "#n قطعات", ur: "#n ٹریکز",
			fi: "#n kappaletta", th: "#n แทร็ก", nb: "#n spor", ms: "#n lagu", hi: "#n ट्रैक्स",
			bn: "#n ট্র্যাক", eo: "#n kantoj", jbo: "#n selsa'a", tok: "#n kalama musi"
		}[language] || " tracks").replace("#n", localizer.processNumber(n));
		if (localizer.rtl[language]) element.setAttribute("dir", "rtl");
	},
